       BLOCK "040904e4"
       BEGIN
           VALUE "CompanyName", "Yahoo! Inc."
           VALUE "FileDescription", "BrowserPlus Control Panel"
           VALUE "FileVersion", "<%= $cmake["VersionString"] %>.0"
           VALUE "LegalCopyright", "Copyright (c) 2007-2008 Yahoo! Inc.  All rights reserved."
           VALUE "InternalName", "BrowserPlusPrefs.exe"
           VALUE "OriginalFilename", "BrowserPlusPrefs.exe"
           VALUE "ProductName", "Y! BrowserPlus"
           VALUE "ProductVersion", "<%= $cmake["VersionString"] %>.0"
       END
   END
   BLOCK "VarFileInfo"
   BEGIN
       VALUE "Translation", 0x409, 1252
   END
END
   