FONT 8, "MS Shell Dlg", 400, 0, 0x1
BEGIN
   CONTROL "", IDC_IE, "{8856F961-340A-11D0-A96B-00C04FD705A2}", WS_TABSTOP,
           0, 0, 400, 350
